num1 = int(input ("num1: "))
num2 = int(input ("num2: "))

# Print the addition of num1 and num2
print("Addition of " f'{num1} and {num2} = {num1 + num2}')
# Print the subtraction of num1 and num2
print("Subtraction of " f'{num1} and {num2} = {num1 - num2}')
# Print the multiplication of num1 and num2
print("Multiplication of " f'{num1} and {num2} = {num1 * num2}')
# Print the division of num1 and num2
print("Division of " f'{num1} and {num2} = {num1 / num2}')
